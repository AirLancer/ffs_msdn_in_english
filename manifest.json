{
  "manifest_version": 2,

  "name": "FFS MSDN in English",
  "description": "This will change the local of MSDN pages to en-us",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
	},
  "permissions": [
    "activeTab", "*://msdn.microsoft.com/*", "*://*.msdn.microsoft.com/*",
	"storage",
	"tabs"
  ],
  "background":{
	"scripts": ["redirectToEnUs.js"],
	"persistent": false
  }
}