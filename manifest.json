{
  "manifest_version": 2,

  "name": "FFS MSDN in English",
  "description": "This will change the local of MSDN pages to English (en-us).",
  "version": "1.3",
  "icons": {
    "16": "icon_16.png",
    "24": "icon_24.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png",
    "256": "icon_256.png",
    "512": "icon_512.png"
	},
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://msdn.microsoft.com/*", 
    "*://*.msdn.microsoft.com/*"
  ],
  "background":{
  "scripts": ["redirectToEnUs.js"]
  }
}