{
  "manifest_version": 2,

  "name": "FFS MSDN in English",
  "description": "This will change the local of MSDN pages to en-us",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png"
	},
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://msdn.microsoft.com/*", 
    "*://*.msdn.microsoft.com/*",
    "storage"
  ],
  "background":{
  "scripts": ["redirectToEnUs.js"]
  }
}